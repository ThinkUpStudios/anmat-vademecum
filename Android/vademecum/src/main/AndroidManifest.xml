<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thinkupstudios.anmat.vademecum" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme" >
        <activity
            android:name="DetalleMedicamentoListActivity"
            android:parentActivityName="MainMenuActivity"
            android:label="@string/app_name" />
        <activity
            android:name="DetalleMedicamentoDetailActivity"
            android:label="@string/title_detallemedicamento_detail"
            >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.thinkupstudios.anmat.vademecum.MainMenuActivity" />
        </activity>
        <activity
            android:name="MainMenuActivity"
            android:label="@string/title_activity_main_menu" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="BusquedaMedicamentoActivity"
            android:parentActivityName="MainMenuActivity"
            android:label="@string/title_activity_busqueda_medicamento"/>
    </application>

</manifest>
