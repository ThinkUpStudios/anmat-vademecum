<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thinkupstudios.anmat.vademecum" >

    <application
        android:name=".aplicacion.MiAplicacion"
        android:allowBackup="true"
        android:icon="@drawable/logo_app"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme" >
        <activity
            android:name=".DetalleMedicamentoListActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MainMenuActivity" />
        <activity
            android:name=".DetalleMedicamentoDetailActivity"
            android:label="@string/title_detallemedicamento_detail" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.thinkupstudios.anmat.vademecum.MainMenuActivity" />
        </activity>
        <activity
            android:name=".MainMenuActivity"
            android:label="@string/title_activity_main_menu" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BusquedaMedicamentoActivity"
            android:label="@string/title_activity_busqueda_medicamento"
            android:parentActivityName=".MainMenuActivity" />
        <activity
            android:name=".AcercaDeActivity"
            android:label="@string/title_activity_ecerca_de"
            android:parentActivityName=".MainMenuActivity" />
        <activity
            android:name=".InformacionActivity"
            android:label="@string/title_activity_info"
            android:parentActivityName=".MainMenuActivity" />
        <activity
            android:name=".BuscarPrincipioActivoActivity"
            android:label="@string/title_activity_buscar_principio_activo"
            android:parentActivityName=".MainMenuActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.thinkupstudios.anmat.vademecum.MainMenuActivity" />
        </activity>
        <activity
            android:name=".DetallePrincipioActivoActivity"
            android:label="@string/title_activity_detalle_principio_activo"
            android:parentActivityName=".BuscarPrincipioActivoActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.thinkupstudios.anmat.vademecum.BuscarPrincipioActivoActivity" />
        </activity>
    </application>

</manifest>
