<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
		<add key="AnmatDataServiceUrl" value="http://localhost/anmatdataservice/AnmatDataService.svc"/>
		<add key="DocumentsPath" value="C:\Anmat\Data" />
		<add key="SourceDatabaseConnectionString" value="mongodb://localhost"/>
		<add key="SourceDatabaseName" value="AnmatSource" />
		<add key="TargetDatabaseName" value="anmat" />
		<add key="TargetMedicinesTableName" value="medicamentos" />
		<add key="TargetActiveComponentsTableName" value="principios_activos" />
		<add key="ReplaceExistingTargetDatabase" value="true" />
		<add key="FullInitialize" value="false" />
		<add key="DefaultTextEncoding" value="ISO-8859-1" />
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5"/>
  </system.web>
  <system.serviceModel>
		<services>
			<service name="Anmat.Server.DataService.AnmatDataService"
                behaviorConfiguration="AnmatDataServiceBehavior">
				<endpoint address=""
                  binding="webHttpBinding"
                  contract="Anmat.Server.DataService.IAnmatDataService"
                   behaviorConfiguration="AnmatDataServiceEndpointBehavior"/>
			</service>
		</services>
    <behaviors>
			<serviceBehaviors>
				<behavior name="AnmatDataServiceBehavior">
					<serviceMetadata httpGetEnabled="true"/>
					<serviceDebug includeExceptionDetailInFaults="true"/>
				</behavior>
			</serviceBehaviors>
			<endpointBehaviors>
				<behavior name="AnmatDataServiceEndpointBehavior">
					<webHttp/>
				</behavior>
			</endpointBehaviors>
    </behaviors> 
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true" multipleSiteBindingsEnabled="true" />
  </system.serviceModel>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true"/>
    <directoryBrowse enabled="true"/>
  </system.webServer>
</configuration>
